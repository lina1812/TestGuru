<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to t('helpers.links.actions.show'), admin_test_path(test) %></td>
  <td><%= link_to t('helpers.links.actions.edit'), edit_admin_test_path(test) %></td>
  
  <td>
    <%= link_to t('helpers.links.actions.delete'),
     admin_test_path(test),
     method: :delete,
     data: { confirm: t('helpers.links.actions.confirm')} %>
  </td>
</tr>
